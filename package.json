{
  "name": "web-starter",
  "version": "1.0.0",
  "description": "Simple Web Starter for React, supports Javascript and Typescript",
  "main": "index.js",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html",
    "check-types": "tsc --noEmit -p .",
    "tlint": "tslint -p tsconfig.json -c tslint.json 'src/**/*.tsx' 'src/**/*.ts'",
    "elint": "eslint src/**/*.js",
    "lint": "yarn tlint && yarn elint",
    "check-all": "yarn check-types && yarn lint",
    "fix": "yarn elint --fix && yarn tlint --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "starter",
    "parcel",
    "react",
    "typescript"
  ],
  "author": "Pervez Iqbal",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-router": "^4.4.3",
    "@types/react-router-dom": "^4.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-root-import": "^6.1.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.0",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.11.0",
    "prettier": "^1.15.3",
    "react-testing-library": "^5.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bulma": "^0.7.2",
    "date-fns": "^1.30.1",
    "formik": "^1.4.1",
    "immer": "^1.9.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "rxjs": "^6.3.3",
    "tcomb": "^3.2.29",
    "ulid": "^2.3.0",
    "unstated": "^2.1.1",
    "yup": "^0.26.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check-all"
    }
  }
}
